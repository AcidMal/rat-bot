# Discord Bot Configuration
# Get your bot token from https://discord.com/developers/applications
DISCORD_TOKEN=your_discord_token_here

# Optional: Set a specific guild ID for testing
GUILD_ID=your_guild_id_here

# Database Configuration
# Format: postgresql://username:password@host:port/database_name
DATABASE_URL=postgresql://user:password@localhost/ratbot

# Lavalink Configuration
# These are the default Lavalink settings
LAVALINK_HOST=localhost
LAVALINK_PORT=2333
LAVALINK_PASSWORD=youshallnotpass

# Bot Configuration
# Command prefix for the bot
PREFIX=!

# Embed color in hexadecimal format
EMBED_COLOR=0x00ff00

# Logging Configuration
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# ModLog Configuration
# Channel ID where moderation logs will be sent
MODLOG_CHANNEL_ID=your_modlog_channel_id_here 